{
  "family": "odm-ecs",
  "containerDefinitions": [
      {
          "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
          "name": "odm-ecs",
          "cpu": 10,
          "memory": 256,
          "essential": true,
          "portMappings": [
              {
                  "containerPort": 8088,
                  "hostPort": 80
              }
          ]
      }
  ]
}